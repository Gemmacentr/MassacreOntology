    <owl:Class rdf:about="http://www.semanticweb.org/mO/ontologies/2020/10/mO#Massacre">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mO/ontologies/2020/10/mO#Event"/>
