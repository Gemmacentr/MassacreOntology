    <!-- http://www.semanticweb.org/mO/ontologies/2020/10/mO#Agent -->


    <owl:Class rdf:about="http://www.semanticweb.org/mO/ontologies/2020/10/mO#Agent">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
